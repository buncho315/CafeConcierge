 <div class='row'>
    <div class='col-md-3'></div>
        <p id= "notice"></p>
        <div class='col-md-8 offset-md-1'>
            <h1>会員一覧</h1>
            <table class = "table table-hover table-inverse">
              <thead>
                <tr>
                  <th>投稿者名</th>
                  <th>コメント数</th>
                </thead>
              <tbody>
                <% @end_users.each do |end_user| %>
                <tr>
                  <td><%= link_to end_user.name, admin_end_user_path(end_user) %><td>
                    <% end_user.shops.each do |shop| %>
                  <td><%= link_to "#{shop.comments.count}コメント", public_shop_comments_path(shop.id) %></td>
                    <% end %>
                    <!--#コメントにリンクは不要-->
                </tr>
                <% end %>
              </tbody>
            </table>
        </div>
</div>