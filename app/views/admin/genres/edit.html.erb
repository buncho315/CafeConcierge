<h1>ジャンル編集ページ</h1>
 <%= form_with model: @genre,  url: admin_genre_path(@genre)  do |f| %>
  <%= f.radio_button :is_enabled, :true%>
  <%= f.label :is_enabled, "有効", {value: :true, style: "display: inline-block;"} %>
  <%= f.radio_button :is_enabled, :false %>
  <%= f.label :is_enabled, "無効", {value: :false, style: "display: inline-block;"} %>
  <%= f.submit "submit"%>
<% end %>
