<div class='container'>
  <div class='row'>
    <div class='col-md-3'></div>
        <p id= "notice"></p>
        <div class='col-md-8 offset-md-1'>
          <h1>ユーザーマイページ情報</h1>
          <table class = "table table-hover table-inverse">
          <%#= @end_user.name %>
           <!--<%#= image_tag @end_user.get_profile_image(100,100) %>あとで写真投稿機能追加する-->
            <thead>
              <tr>
                <th>ユーザーの投稿一覧</th>
              </tr>
            </thead>
            <tbody>
              <tr>
            <% @shop.each do |shop| %>
                <td>投稿したユーザー名:<%= shop.end_user.name %></td>
                <td>店名:<%= shop.title %></td>
                <td>店舗説明:<%= shop.body %></td>
              </tr>
            </tbody>
            <% end %>
          </table>
        </div>
  </div>
</div>