<div class='container'>
  <div class='row'>
    <div class='col-md-3'></div>
        <p id= "notice"></p>
        <div class='col-md-8 offset-md-1'>
          <h1>ユーザープロフィール編集</h1>
          <table class = "table table-hover table-inverse">
            <tr>
            <%= form_with model: @end_user_session_path do |f| %>
             <td><label for="inputName">登録名</label>
             <%= f. text_field :name, autofocus: true, id:"inputName", placeholder:"登録名"%>
             </td>
             <td><label for="inputImage">プロフィール画像</label>
             <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
             </td>
             <td>
               <%= f.submit "変更を保存" %>
               </td>
            <% end %>
            </tr>
          </table>
        </div>
  </div>
</div>