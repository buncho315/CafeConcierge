<div class='container'>
  <div class='row'>
    <div class='col-md-3'></div>
        <p id= "notice"></p>
        <div class='col-md-8 offset-md-1'>
          <h1>新規投稿画面</h1>
            <%= form_with model: @shop, url: public_shops_path do |f| %>
          <h4>店名</h4>
            <%= f.text_field :title %>
          <h4>店舗説明(300文字以内)</h4>
            <%= f.text_area :body %>
            <%= f.submit '投稿する' %>
            <% end %>
        </div>
  </div>
</div>